{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p ${PORT:-3332}",
    "build": "next build",
    "start": "next start -p ${PORT:-3332}",
    "lint": "next lint --dir src",
    "lint:fix": "yarn lint --fix",
    "codegen": "graphql-codegen --config codegen.yml",
    "format": "prettier --write --ignore-path .gitignore './**/*.{js,jsx,ts,tsx,json}'"
  },
  "dependencies": {
    "graphql-tag": "^2.12.6",
    "next": "12.3.1",
    "next-urql": "^4.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-is": "^18.2.0",
    "urql": "^3.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.5",
    "@graphql-codegen/client-preset": "1.0.5",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-urql": "^3.7.0",
    "@types/node": "18.7.23",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.24.0",
    "eslint-config-next": "12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "graphql": "^16.6.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  }
}
